<odoo>
    <record id="view_indications_tree" model="ir.ui.view">
        <field name="name">dynamed.indications.tree</field>
        <field name="model">dynamed.indications</field>
        <field name="arch" type="xml">
            <tree create="false">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_indications_form" model="ir.ui.view">
        <field name="name">dynamed.indications.form</field>
        <field name="model">dynamed.indications</field>
        <field name="arch" type="xml">
            <form create="false">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="action_indications" model="ir.actions.act_window">
        <field name="name">indications</field>
        <field name="res_model">dynamed.indications</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>