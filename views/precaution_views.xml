<odoo>
    <record id="view_precaution_tree" model="ir.ui.view">
        <field name="name">precaution.tree</field>
        <field name="model">dynamed.precaution</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_precaution_form" model="ir.ui.view">
        <field name="name">precaution.form</field>
        <field name="model">dynamed.precaution</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_precaution" model="ir.actions.act_window">
        <field name="name">Pr√©cautions</field>
        <field name="res_model">dynamed.precaution</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>